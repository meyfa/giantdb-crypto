  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.43",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "c8": "7.11.3",
    "chai": "4.3.6",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "giantdb": "3.1.0",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  }
